
    <!-- user card -->
    <div class="user-container">
        <div class="profile">
            <ion-icon name="settings-outline" class="settings"></ion-icon>            
            <ion-icon name="menu-outline" class ="menu"></ion-icon>
            <img src="../images/profilepicture.png" class="profile-picture">
            <h3 class ="name" >{{ userData.username }}</h3>
            <p class="user-desc">Selling and Buying Cups!</p>
            <div class="social-media">
                <ion-icon name="logo-instagram" class="instagram"></ion-icon>
                <ion-icon name="logo-facebook" class="facebook"></ion-icon>
                <ion-icon name="logo-discord" class="discord"></ion-icon>
            </div>
            <button type="button" class="followBtn">Follow</button>
            <div class="profile-bottom">
                <p><a href="#user-cups">Current Cups!</a></p>
                <ion-icon name="arrow-down-outline" class="arrow-down"></ion-icon>
            </div>
        </div>
    </div>

